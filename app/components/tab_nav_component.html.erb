<%= tag.nav(class: "flex justify-between border-b border-gray-400 mb-8") do %>
  <%= tag.div(class: "-mb-px flex flex-wrap space-x-2 md:flex-nowrap") do %>
    <% tabs.each do |tab| %>
      <%= tab %>
    <% end %>
  <% end %>

  <%= tag.div(class: "-mb-px flex flex-wrap space-x-2 md:flex-nowrap") do %>
    <%= extra %>
  <% end %>
<% end %>
